{
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/main.py"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_KEY": "@supabase_key",
    "SUPABASE_SERVICE_KEY": "@supabase_service_key",
    "ENVIRONMENT": "production",
    "API_HOST": "0.0.0.0",
    "FRONTEND_URL": "@frontend_url"
  }
}
